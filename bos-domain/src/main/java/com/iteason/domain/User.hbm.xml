        </id>
        <property name="username" >
        </property>
        <property name="password" >
        </property>
        <property name="salary" >
        </property>
        <property name="birthday" >
        </property>
        <property name="gender" >
        </property>
        <property name="station" >
        </property>
        <property name="telephone" >
        </property>
        <property name="remark" >
        </property>
    </class>
</hibernate-mapping>
